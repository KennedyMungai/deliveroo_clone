{
	"expo": {
		"name": "Deliveroo Clone",
		"slug": "deliveroo_clone",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/deliveroo_icon.png",
		"scheme": "myapp",
		"userInterfaceStyle": "automatic",
		"splash": {
			"image": "./assets/images/deliveroo_icon.png",
			"resizeMode": "contain",
			"backgroundColor": "#00ff7f"
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/deliveroo_icon.png",
				"backgroundColor": "#00ff7f"
			},
			"package": "com.kennedymungai.deliveroo_clone"
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/deliveroo_icon.png"
		},
		"plugins": ["expo-router"],
		"experiments": {
			"typedRoutes": true
		}
	}
}
